{
  "name": "eslint-plugin-ferns",
  "version": "0.5.0",
  "description": "ESLint plugin for full stack Express/React Native/Node.js applications",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --ext .js ./",
    "lint:watch": "esw --ext .js --watch .",
    "test": "mocha tests/**/*.js",
    "test:debug": "mocha --inspect-brk tests/**/*.js",
    "test:watch": "mocha tests/**/*.js -w",
    "test:coverage": "istanbul cover --dir reports/coverage node_modules/mocha/bin/_mocha tests/**/*.js -- --reporter dot"
  },
  "keywords": [
    "react",
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "Josh Gachnang <josh@nang.io>",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/FlourishHealth/eslint-plugin-ferns.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "babel-eslint": "^7.1.0",
    "eslint": "^9.18.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "eslint-watch": "^8.0.0",
    "istanbul": "^0.4.5",
    "mocha": "^10.2.0",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint-config-expo": "^10.0.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-comment-length": "^2.0.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-lodash": "^8.0.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-native": "^5.0.0",
    "eslint-plugin-react-perf": "^3.3.3",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-unused-imports": "^4.0.0",
    "prettier": "^3.4.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "eslint": "^9.18.0",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint-config-expo": "^10.0.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-comment-length": "^2.0.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-lodash": "^8.0.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-native": "^5.0.0",
    "eslint-plugin-react-perf": "^3.3.3",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-unused-imports": "^4.0.0",
    "prettier": "^3.4.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.18.0"
  }
}
